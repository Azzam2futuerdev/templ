{{~ end ~}}
{{~ if Input.primary ~}}
- primary.txt: 8dff8e3ac78e9f34fb181476f120526b
  level: primary
{{~ end ~}}
{{~ if Input.secondary ~}}
- secondary.txt: 8dff8e3ac78e9f34fb181476f120526b
  level: secondary
{{~ end ~}}
